@using Metal_Archives.Models;
@model List<BandModel>
@{
    ViewBag.Title = "Bands";
}

<h2>Bands</h2>
<table style="width:100%">
    <tr>
        <th>Bandname</th>
        <th>Country</th>
        <th>Genre</th>
        <th>Status</th>
    </tr>

    <tbody></tbody>

    @foreach (BandModel band in Model)
    {
        <tr>
            <td><a href="@Url.Action("Band" , "Band",new {bandname = band.Name})"> @band.Name</a></td>
            <td>@band.Country </td>
            <td>@band.Genre </td>
            <td>@band.Status </td>
        </tr>
    }
</table>
   
     
